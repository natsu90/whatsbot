{
	"name": "laravel/laravel",
	"description": "The Laravel Framework.",
	"keywords": ["framework", "laravel"],
	"license": "MIT",
	"type": "project",
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/mgp25/WhatsAPI-Official"
        },
        {
        	"type": "git",
            "url": "https://github.com/natsu90/ChatterBotApiPhp"
        },
        {
        	"type": "git",
        	"url": "https://github.com/Derp-com-my/laravel-internal-queue"
        },
        {
        	"type": "git",
        	"url": "https://github.com/natsu90/laravel-async-queue"
        }
    ],
	"require": {
		"laravel/framework": "4.2.*",
		"mgp25/whatsapi": "dev-master#70330961a14db6bf3625e7634b0c3fb3b574bec9",
		"jenssegers/mongodb": "*",
		"Schumix/ChatterBotApi": "dev-master",
		"armxy/laravel-internal-queue": "dev-master",
		"barryvdh/laravel-async-queue": "dev-master",
		"ext-mongo": "*",
		"ext-mbstring": "*",
		"vlucas/phpdotenv": "~1.1"
	},
	"autoload": {
		"classmap": [
			"app/commands",
			"app/controllers",
			"app/models",
			"app/database/migrations",
			"app/database/seeds",
			"app/tests/TestCase.php"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "stable"
}
